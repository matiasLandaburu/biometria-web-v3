{
  "name": "@link-ts/biometria",
  "version": "1.0.0",
  "main": "./dist/lib/index.js",
  "types": "./dist/types/index.d.ts",
  "private": true,
  "scripts": {
    "start": "cypress open",
    "build": "webpack && tsc",
    "build:release": "npm run lint && npm run build && git add ./dist/bundle.js && git commit -m \"Nuevo bundle\"",
    "clean": "rimraf ./coverage ./dist ./docs",
    "lint": "tslint --project .",
    "release:patch": "npm run build:release && npm version patch && npm run release:publish",
    "release:minor": "npm run build:release && npm version minor && npm run release:publish",
    "release:major": "npm run build:release && npm version major && npm run release:publish",
    "release:publish": "copy-partial-json -s ./package.json  -t ./dist/package.json -k version && npm publish ./dist && git push && git push --tags",
    "test": "cypress run -b chrome -s ./src/lib/tests/index.spec.ts",
    "docs": "rimraf ./docs && typedoc ./src/lib --out ./docs"
  },
  "license": "ISC",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.9.0",
    "@types/chai": "^4.2.18",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.3",
    "@types/sinon": "^10.0.2",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^5.28.0",
    "chai": "^4.3.4",
    "copy-partial-json": "^1.1.1",
    "copy-webpack-plugin": "^9.0.0",
    "cypress": "^7.7.0",
    "mocha": "^9.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.1",
    "source-map-support": "^0.5.19",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "blob-util": "^2.0.2",
    "dateformat": "^4.5.1",
    "face-api.js": "^0.22.2",
    "http-errors": "^2.0.0",
    "npx": "^10.2.2",
    "ts-date": "^3.0.2",
    "uuid": "^8.3.2"
  }
}
